{
  "name": "custom-three-engine",
  "version": "0.0.1",
  "description": "",
  "private": false,
  "license": "ISC",
  "author": "Rob Doneux",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "main": "./dist/three-engine.umd.cjs",
  "module": "./dist/three-engine.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/three-engine.js",
      "require": "./dist/three-engine.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "publishConfig": {
    "registry": "http://192.168.0.106:4873"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "npm run build && concurrently \"npx ts-node ./build.ts --watch\" \"npx serve ./dist\"",
    "build": "rm -rf ./dist && tsc -b && npx ts-node ./build.ts",
    "publish": "npm run build && npm publish --repository http://192.168.0.106:4873"
  },
  "dependencies": {
    "three": "^0.172.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/three": "^0.172.0",
    "concurrently": "^9.1.2",
    "esbuild": "^0.24.2",
    "esbuild-plugin-copy": "^2.1.1",
    "typescript": "^5.7.3"
  }
}